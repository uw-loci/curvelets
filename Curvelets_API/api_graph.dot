    CircR[label="circ_r\n(Alignment)", shape=ellipse, style=bold, fillcolor=lightblue, fontsize=18, height=0.8, width=1.5];
    CircSkew[label="circ_skewness", shape=ellipse, style=bold, fillcolor=lightblue, fontsize=18, height=0.8, width=1.5];
    CircKurt[label="circ_kurtosis", shape=ellipse, style=bold, fillcolor=lightblue, fontsize=18, height=0.8, width=1.5];
